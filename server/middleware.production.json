{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": {
        "action": "deny"
      }
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubDomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    },
    "./middleware/openstore-middleware": {}
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "paths": [
        "/client/dist"
      ],
      "params": "$!../client/dist"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "./middleware/error-reporter.js": {},
    "strong-error-handler": {
      "params": {
        "debug": false,
        "defaultType": "json",
        "negotiateContentType": false
      }
    }
  }
}