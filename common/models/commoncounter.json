{
  "name": "commoncounter",
  "base": "configentity",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SoftDelete": true,
    "Timestamp": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "notifications": {
      "type": "number"
    },
    "createdby": {
      "type": "string"
    },
    "modifiedby": {
      "type": "string"
    },
    "cart": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find"
    }
  ],
  "methods": {}
}
